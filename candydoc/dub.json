{
	"authors": ["SHOO"],
	"copyright": "Copyright Â© 2019, SHOO",
	"description": "Document generator using Ddoc D language standard feature for dub package.",
	"name": "candydoc",
	"importPaths": [
		"."
	],
	"configurations": [
		{
			"name": "default",
			"targetType": "executable",
			"dependencies": {
				"gendoc": {"path": ".."}
			},
			"mainSourceFile": "source/app.d",
			"subConfigurations": {
				"gendoc": "library"
			}
		},
		{
			"name": "init",
			"targetType": "none",
			"dependencies": {
				"none": {"path": "../none"}
			},
			"copyFiles": [
				"ddoc",
				"source_docs"
			]
		},
		{
			"name": "unittest",
			"targetType": "executable",
			"dependencies": {
				"gendoc": {"path": ".."}
			},
			"subConfigurations": {
				"gendoc": "library"
			},
			"mainSourceFile": "source/app.d",
			"sourceFiles": ["../.github/ut.d"]
		}
	]
}
