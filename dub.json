	"copyright": "Copyright Â© 2019, SHOO",
	"description": "Document generator using Ddoc D language standard feature for dub package.",
	"license": "BSL-1.0",
	"name": "gendoc",
	"dependencies": {
		"dub": "~>1.16.0",
		"mustache-d": "~>0.1.3"
	},
	"subConfigurations": {
		"dub": "library"
	},
	"importPaths": [
		"."
	],
	"configurations": [
		{
			"name": "default",
			"targetType": "executable"
		},
		{
			"name": "init",
			"targetType": "none",
			"copyFiles": [
				"ddoc",
				"source_docs"
			]
		}
	]
}
